{
  "name": "brytex_website",
  "version": "0.0.1",
  "description": "A website for Brytex",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "start": "run-p dev:*",
    "build": "run-s prod:*",
    "clean": "rimraf ./_site ./.cache",
    "dev:eleventy": "eleventy --serve",
    "dev:parcel": "parcel watch ./_assets/js/main.js --out-dir ./_site/assets",
    "prod:eleventy": "eleventy",
    "prod:parcel": "parcel build ./_assets/js/main.js --out-dir ./_site/assets"
  },
  "author": "Warren Stix",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "bootstrap": "^5.1.3",
    "eleventy-plugin-lazyimages": "^2.1.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3"
  }
}
